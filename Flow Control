#6 
def upper_words(greeting):
  if len(greeting) > 10:
    return greeting.upper()
  else:
    return greeting

print(upper_words("hello world"))
print(upper_words("goodbye"))

#7 
def number_range(number):
  if number < 0:
    print(f"{number} is less than 0")
  elif number < 50 and > 0:
    print(f"{number} is between 0 and 50")
  elif number < 100 and > 50:
    print(f"{number} is between 50 and 100")
  elif number > 100:
    print(f"{number} is greater than 100")

number_range(-1)
number_range(36)
number_range(60)
number_range(150)
